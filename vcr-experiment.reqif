          </TYPE>
        </SPEC-OBJECT>
        <SPEC-OBJECT IDENTIFIER="REQUIREMENT-dd627227-1edf-4aae-9114-54cc40815a0e">
          <VALUES>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="AGW-S-009">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.ForeignID</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="Medium">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>CRITICALITY</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="Preserves Performance">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.Name</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="The device SHALL be scoped to sufficient detail to preserve network domain performance guarantees in *TND*.">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.Text</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="Inspection of vendor reliability testing reports to confirm that the device will not cause loss of performance in TND.\n\nAND\n\nAll the verification steps of all the applicable derivative requirements.">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>VERIFICATION</ATTRIBUTE-DEFINITION-STRING-REF>
