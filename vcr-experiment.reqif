        </SPEC-OBJECT>
        <SPEC-OBJECT IDENTIFIER="REQUIREMENT-fa21bdaf-6717-4952-9596-50fb00eef150">
          <VALUES>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="SAA-050">
              <DEFINITION>
                <ATTRIBUTE-DEFINITION-STRING-REF>ReqIF.ForeignID</ATTRIBUTE-DEFINITION-STRING-REF>
              </DEFINITION>
            </ATTRIBUTE-VALUE-STRING>
            <ATTRIBUTE-VALUE-STRING THE-VALUE="Security Management">
